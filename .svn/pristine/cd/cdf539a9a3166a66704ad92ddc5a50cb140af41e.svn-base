function showdd(obj,im,im1){
	  var dp=obj.parentNode;
	  var dpd=dp.getElementsByTagName("dd");
	  var img=obj.getElementsByTagName("div");
	 var imgi=img[1].getElementsByTagName("img");
	 var ima=img[0].getElementsByTagName("img");
	if(dpd[0].style.display=='none')
	{dpd[0].style.display="block";
	imgi[0].src="/cbtconsole/img/index/catr1.png";
	img[0].style.color="#ff7800";
	ima[0].src="/cbtconsole/img/index/cate/"+im;
	}
	
	else 
	{  
		dpd[0].style.display="none";
	imgi[0].src="/cbtconsole/img/index/catr.png";
	img[0].style.color="#4e4d4e";
	ima[0].src="/cbtconsole/img/index/cate/"+im1;
	}
	  
}
