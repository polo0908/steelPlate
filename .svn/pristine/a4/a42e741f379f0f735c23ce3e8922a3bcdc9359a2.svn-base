.form-control {
	outline: none;
	display: block;
	width: 100%;
	padding: 3px 0px 3px 0px;
	font-size: 12px;
	line-height: 22.5px;
	color: #555;
	background-color: #fff;
	background-image: none;
	resize: none;
	overflow: hidden;
	border: none;
	height: 120px;
	/*	margin-top: -10px;*/
	transition: border-color ease-in-out .15s, box-shadow ease-in-out .15s;
}